       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"Christoffel", "[", 
          RowBox[{
          "g", ",", " ", "\[Alpha]", ",", " ", "\[Beta]", ",", " ", "\[Nu]", 
           ",", " ", "coords"}], "]"}], ",", " ", 
         RowBox[{"coords", "[", 
          RowBox[{"[", "\[Mu]", "]"}], "]"}]}], "]"}], "-", 
       "\[IndentingNewLine]", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"Christoffel", "[", 
          RowBox[{
          "g", ",", " ", "\[Alpha]", ",", " ", "\[Beta]", ",", " ", "\[Mu]", 
           ",", " ", "coords"}], "]"}], ",", " ", 
         RowBox[{"coords", "[", 
          RowBox[{"[", "\[Nu]", "]"}], "]"}]}], "]"}], "+", 
       "\[IndentingNewLine]", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Christoffel", "[", 
           RowBox[{
           "g", ",", " ", "\[Alpha]", ",", " ", "\[Sigma]", ",", " ", "\[Mu]",
             ",", " ", "coords"}], "]"}], 
          RowBox[{"Christoffel", "[", 
           RowBox[{
           "g", ",", " ", "\[Sigma]", ",", " ", "\[Beta]", ",", " ", "\[Nu]", 
            ",", " ", "coords"}], "]"}]}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"\[Sigma]", ",", " ", "1", ",", " ", 
           RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}], "-", 
       "\[IndentingNewLine]", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Christoffel", "[", 
           RowBox[{
           "g", ",", " ", "\[Alpha]", ",", " ", "\[Sigma]", ",", " ", "\[Nu]",
             ",", " ", "coords"}], "]"}], 
          RowBox[{"Christoffel", "[", 
           RowBox[{
           "g", ",", " ", "\[Sigma]", ",", " ", "\[Beta]", ",", " ", "\[Mu]", 
