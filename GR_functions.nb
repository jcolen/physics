  RowBox[{
   RowBox[{"RiemannTensor", "[", 
    RowBox[{
    "g_", ",", " ", "\[Alpha]_", ",", " ", "\[Beta]_", ",", " ", "\[Mu]_", 
     ",", " ", "\[Nu]_", ",", " ", "coords_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[Sigma]", "}"}], ",", "\[IndentingNewLine]", 
