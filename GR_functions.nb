  3.7601309854099703`*^9}},ExpressionUUID->"62a5e403-dbab-44bc-bb5d-\
fd7c6313f2db"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CovariantDerivative", "[", 
    RowBox[{
    "V_", ",", " ", "\[Alpha]_", ",", " ", "\[Beta]_", ",", " ", "g_", ",", 
     " ", "coords_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[Mu]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Simplify", "[", 
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"V", "[", 
          RowBox[{"[", "\[Alpha]", "]"}], "]"}], ",", " ", 
         RowBox[{"coords", "[", 
          RowBox[{"[", "\[Beta]", "]"}], "]"}]}], "]"}], "+", 
       "\[IndentingNewLine]", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"V", "[", 
           RowBox[{"[", "\[Mu]", "]"}], "]"}], 
          RowBox[{"Christoffel", "[", 
           RowBox[{
           "g", ",", " ", "\[Alpha]", ",", " ", "\[Mu]", ",", " ", "\[Beta]", 
            ",", " ", "coords"}], "]"}]}], ",", " ", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"\[Mu]", ",", " ", "1", ",", " ", 
           RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}]}], 
      "]"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7600337763103733`*^9, 3.760033938304221*^9}, 
   3.760129714782584*^9, {3.760129788061634*^9, 3.760129791186554*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"f34fd54d-6078-439c-ba77-ac9f535cc965"],

