Cell[BoxData[
 RowBox[{
  RowBox[{"PrintChristoffelSymbols", "[", 
   RowBox[{"g_", ",", " ", "coords_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", " ", "\[Beta]", ",", " ", "\[Gamma]", ",", " ", "c", 
      ",", " ", "sym1", ",", " ", "sym2"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Do", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"c", " ", "=", " ", 
             RowBox[{"Christoffel", "[", 
              RowBox[{
              "g", ",", " ", "\[Alpha]", ",", " ", "\[Beta]", ",", " ", 
               "\[Gamma]", ",", " ", "coords"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"sym1", " ", "=", " ", 
             RowBox[{"Subscript", "[", 
              RowBox[{
               RowBox[{"Subscript", "[", 
                RowBox[{
                 RowBox[{"Superscript", "[", 
                  RowBox[{"\[CapitalGamma]", ",", 
                   RowBox[{"coords", "[", 
                    RowBox[{"[", "\[Alpha]", "]"}], "]"}]}], "]"}], ",", " ", 
                 
                 RowBox[{"coords", "[", 
                  RowBox[{"[", "\[Beta]", "]"}], "]"}]}], "]"}], ",", " ", 
               RowBox[{"coords", "[", 
                RowBox[{"[", "\[Gamma]", "]"}], "]"}]}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"sym2", " ", "=", " ", 
             RowBox[{"Subscript", "[", 
              RowBox[{
               RowBox[{"Subscript", "[", 
                RowBox[{
                 RowBox[{"Superscript", "[", 
                  RowBox[{"\[CapitalGamma]", ",", " ", 
                   RowBox[{"coords", "[", 
                    RowBox[{"[", "\[Alpha]", "]"}], "]"}]}], "]"}], ",", " ", 
                 
                 RowBox[{"coords", "[", 
                  RowBox[{"[", "\[Gamma]", "]"}], "]"}]}], "]"}], ",", " ", 
               RowBox[{"coords", "[", 
                RowBox[{"[", "\[Beta]", "]"}], "]"}]}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"c", " ", "\[Equal]", " ", "0"}], ",", " ", ",", " ", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"\[Beta]", "\[Equal]", "\[Gamma]"}], ",", " ", 
                RowBox[{"Print", "[", 
                 RowBox[{"sym1", ",", " ", "\"\< = \>\"", ",", " ", "c"}], 
                 "]"}], ",", " ", 
                RowBox[{"Print", "[", 
                 RowBox[{
                 "sym1", ",", " ", "\"\< = \>\"", ",", " ", "sym2", ",", " ", 
                  "\"\< = \>\"", ",", " ", "c"}], "]"}]}], "]"}], ",", " ", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"\[Beta]", "\[Equal]", "\[Gamma]"}], ",", " ", 
                RowBox[{"Print", "[", 
                 RowBox[{"sym1", ",", " ", "\"\< = \>\"", ",", " ", "c"}], 
                 "]"}], ",", " ", 
                RowBox[{"Print", "[", 
                 RowBox[{
                 "sym1", ",", " ", "\"\< = \>\"", ",", " ", "sym2", ",", " ", 
                  "\"\< = \>\"", ",", " ", "c"}], "]"}]}], "]"}]}], "]"}], 
            ";"}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"\[Gamma]", ",", " ", "\[Beta]", ",", " ", 
             RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"\[Beta]", ",", " ", "1", ",", " ", 
           RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"\[Alpha]", ",", " ", "1", ",", " ", 
         RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.7600340569997587`*^9, 3.760034314455917*^9}, {
