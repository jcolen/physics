Cell[TextData[{
 "Christoffel Symbol Calculation: \n\t@param g - The spacetime metric\n\t\
@param \[Gamma], \[Beta], \[Mu] - Indices for the Christoffel symbol\n\t\
@param coords - A list of coordinates such that coords[[\[Alpha]]] = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "\[Alpha]"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ed28b99a-4719-4e04-b5f2-2a4deca7786b"],
 "\n\t@return Christoffel Symbol ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    SuperscriptBox["\[CapitalGamma]", "\[Gamma]"], "\[Beta]\[Mu]"], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "77a1f4fd-88e8-4527-82de-c0cf064b8364"]
}], "Text",
 CellChangeTimes->{{3.760033549677518*^9, 
  3.760033722261486*^9}},ExpressionUUID->"ca1da0f5-34e1-48e1-beea-\
60e67f86bff3"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Christoffel", "[", 
    RowBox[{
    "g_", ",", " ", "\[Gamma]_", ",", " ", "\[Beta]_", ",", " ", "\[Mu]_", 
     ",", " ", "coords_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Alpha]", ",", " ", "gi"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gi", " ", "=", " ", 
       RowBox[{"Inverse", "[", "g", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Simplify", "[", 
       RowBox[{
        FractionBox["1", "2"], 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"gi", "[", 
            RowBox[{"[", 
             RowBox[{"\[Alpha]", ",", " ", "\[Gamma]"}], "]"}], "]"}], 
           "\[IndentingNewLine]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"g", "[", 
                RowBox[{"[", 
                 RowBox[{"\[Alpha]", ",", " ", "\[Beta]"}], "]"}], "]"}], ",",
                " ", 
               RowBox[{"coords", "[", 
                RowBox[{"[", "\[Mu]", "]"}], "]"}]}], "]"}], "+", 
             "\[IndentingNewLine]", 
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"g", "[", 
                RowBox[{"[", 
                 RowBox[{"\[Alpha]", ",", " ", "\[Mu]"}], "]"}], "]"}], ",", 
               " ", 
               RowBox[{"coords", "[", 
                RowBox[{"[", "\[Beta]", "]"}], "]"}]}], "]"}], "-", 
             "\[IndentingNewLine]", 
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{"g", "[", 
                RowBox[{"[", 
                 RowBox[{"\[Beta]", ",", " ", "\[Mu]"}], "]"}], "]"}], ",", 
               " ", 
               RowBox[{"coords", "[", 
                RowBox[{"[", "\[Alpha]", "]"}], "]"}]}], "]"}]}], ")"}]}], 
          ",", " ", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"\[Alpha]", ",", " ", "1", ",", " ", 
            RowBox[{"Length", "[", "coords", "]"}]}], "}"}]}], "]"}]}], 
       "]"}]}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.760032668461549*^9, 3.760032720507807*^9}, {
   3.760032776613946*^9, 3.7600327814933043`*^9}, {3.7600328163238983`*^9, 
